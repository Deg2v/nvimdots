{
	"plot2": {
		"prefix": "plott2",
		"body": [
			"import numpy as np",
			"import matplotlib.pyplot as plt"
		],
		"description": "numpy matplotlib"
	},
	"logging": {
		"prefix": "sn_logg",
		"body": [
			"import logging",
			"ch = logging.StreamHandler()",
			"formatter = logging.Formatter('%(asctime)s [%(name)s] %(levelname)s: %(message)s')",
			"ch.setFormatter(formatter)",
			"logger = logging.getLogger('--hzf--')",
			"if not logger.handlers:   # 解决重复输出",
			"    logger.setLevel(logging.DEBUG)",
			"    logger.addHandler(ch)",
            "    # log 文件",
            "    file_handler = logging.FileHandler(filename='checkerboard.log')",
			"    file_handler.setLevel(logging.DEBUG)",
			"    file_handler.setFormatter(formatter)",
			"    logger.addHandler(file_handler)\n"
		],
		"description": "logger"
	},
	"changelog": {
		"prefix": "change",
		"body": [
            "'''",
			" @Time_change : $CURRENT_YEAR/$CURRENT_MONTH/$CURRENT_DATE $CURRENT_HOUR:$CURRENT_MINUTE:$CURRENT_SECOND",
			" version: $1 description : $2",
            "'''"
		],
        "description":"time"
	}
}
